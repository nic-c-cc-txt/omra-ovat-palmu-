{
	"name": "reg",
	"displayName": "REG",
	"description": "Windows Registry Script (.reg) Language package for VSCode",
	"version": "1.0.3",
	"publisher": "ionutvmi",
	"icon": "icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/ionutvmi/reg-vscode"
	},
	"bugs": {
		"url": "https://github.com/ionutvmi/reg-vscode/issues",
		"email": "ionutvmi@gmail.com"
	},
	"engines": {
		"vscode": "^1.0.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "reg",
				"aliases": [
					"REG",
					"reg"
				],
				"extensions": [
					".reg",
					".REG"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "reg",
				"scopeName": "source.reg",
				"path": "./syntaxes/reg.tmLanguage"
			}
		],
		"commands": [
			{
				"command": "extension.reg.jumpToKey",
				"title": "REG: Jump to registry key"
			}
		]
	},
	"activationEvents": [
		"onLanguage:reg"
	],
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.3.1",
		"vscode": "^1.1.28",
		"tslint": "^5.12.1",
		"@types/node": "^10.12.21",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"sudo-prompt": "^9.0.0"
	},
	"__metadata": {
		"id": "9f8192be-78bf-4153-806d-08ae956ce8b0",
		"publisherId": "3a1bd914-dacc-4903-89d9-bd7ff757eb7f",
		"publisherDisplayName": "Mihai Vilcu",
		"installedTimestamp": 1619545543134
	}
}