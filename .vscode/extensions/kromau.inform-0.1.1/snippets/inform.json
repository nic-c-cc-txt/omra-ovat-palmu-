		"body": [
			"DOUT OG#($1) ${2|B0,LB0|}${3:##}",
			"$0"
		],
		"description": "DOUT OG"
	},

	"DIN Bxxx IN": {
		"prefix": "din",
		"body": [
			"DIN B0${1:##} IN#($2)",
			"$0"
		],
		"description": "DIN Bxxx IN"
	},

	"DIN Bxxx IG": {
		"prefix": "dig",
		"body": [
			"DIN B0${1:##} IG#($2)",
