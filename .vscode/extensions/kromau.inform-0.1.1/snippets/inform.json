	"JOB WITH GROUP": {
		"prefix": "job_R1",
		"body": [
		"/JOB",
		"//NAME ${1:${TM_FILENAME_BASE}}",
		"//POS",
		"///NPOS 0,0,0,0,0,0",
		"//INST",
		"///DATE $CURRENT_YEAR/$CURRENT_MONTH/$CURRENT_DATE $CURRENT_HOUR:$CURRENT_MINUTE",
		"///ATTR SC,RW",
		"///GROUP1 ${2|RB1,RB1\\,BS1,RB1\\,ST1,ST1|}",
		"///LVARS 3,3,3,3,0,0,0,0",
		"NOP",
		"$0",
		"END",
		""
		],
		"description": "JOB WITH GROUP"
	},

